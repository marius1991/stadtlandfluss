<h1>New game</h1>

<%= form_for @game do |f| %>
    <div class="field">
      <%= f.label :name %><br>
      <%= f.text_field :name %>
    </div>
    <div class="field">
      <%= f.label :max_round_count %><br>
      <%= f.text_field :max_round_count %>
    </div>
    <div class="actions"><%= f.submit %></div>
<% end %>

<%= link_to 'Back', games_path %>
